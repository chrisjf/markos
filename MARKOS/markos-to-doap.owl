<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY doap "http://usefulinc.com/ns/doap#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY top "http://www.markosproject.eu/ontologies/top#" >
    <!ENTITY markos "http://www.markosproject.eu/ontologies/software#" >
]>


<rdf:RDF xmlns="http://www.markosproject.eu/ontologies/markos-to-doap#"
     xml:base="http://www.markosproject.eu/ontologies/markos-to-doap"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:markos="http://www.markosproject.eu/ontologies/software#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:doap="http://usefulinc.com/ns/doap#"
     xmlns:top="http://www.markosproject.eu/ontologies/top#">
    <owl:Ontology rdf:about="http://www.markosproject.eu/ontologies/markos-to-doap">
        <rdfs:comment xml:lang="en">This ontology describes a partial mapping between the MARKOS ontology and the DOAP ontology. </rdfs:comment>
        <owl:imports rdf:resource="http://usefulinc.com/ns/doap#"/>
        <owl:versionIRI rdf:resource="http://www.markosproject.eu/ontologies/markos-to-doap#2013/07/16"/>
        <owl:imports rdf:resource="&markos;2013/07/16"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://usefulinc.com/ns/doap#release -->

    <rdf:Description rdf:about="&doap;release">
        <owl:equivalentProperty rdf:resource="&markos;releasedSoftware"/>
    </rdf:Description>

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://usefulinc.com/ns/doap#anon-root -->

    <rdf:Description rdf:about="&doap;anon-root">
        <owl:equivalentProperty rdf:resource="&markos;anonymousAccess"/>
    </rdf:Description>
    
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://usefulinc.com/ns/doap#Repository -->

    <rdf:Description rdf:about="&doap;Repository">
        <owl:equivalentClass rdf:resource="&markos;FileRepository"/>
    </rdf:Description>


    


    <!-- http://www.markosproject.eu/ontologies/software#SoftwareProject -->

    <rdf:Description rdf:about="&markos;SoftwareProject">
        <rdfs:subClassOf rdf:resource="&doap;Project"/>
    </rdf:Description>
    


    <!-- http://www.markosproject.eu/ontologies/software#SoftwareRelease -->

    <rdf:Description rdf:about="&markos;SoftwareRelease">
        <rdfs:subClassOf rdf:resource="&doap;Version"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotations
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description rdf:about="&doap;location">
        <top:correspondingSuperProperty rdf:resource="&top;url"/>
    </rdf:Description>
</rdf:RDF>



