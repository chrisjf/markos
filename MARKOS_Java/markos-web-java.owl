    <!-- http://www.markosproject.eu/ontologies/markos-web-java#aplication-name -->

    <owl:DatatypeProperty rdf:about="&mwjava;aplication-name">
        <rdfs:label>aplication-name</rdfs:label>
        <rdfs:comment>By defaults is the bundle name (.ear file name) without the bundle extension. This can be overridden in application.xml. Also,  &lt;application-name&gt; is applicable only if the bean is packaged inside a  .ear file.</rdfs:comment>
        <mjava:relatedTo>EJB Component</mjava:relatedTo>
        <rdfs:subPropertyOf rdf:resource="&mwjava;jndiLookupName"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#bean-name -->

    <owl:DatatypeProperty rdf:about="&mwjava;bean-name">
        <rdfs:label>bean-name</rdfs:label>
        <rdfs:comment>By defaults is the unqualified class name of the bean. However, if @Stateful or @Stateless or @Singleton uses the name attribute, then the value specified there will be used as the bean name.</rdfs:comment>
        <mjava:relatedTo>EJB Componet</mjava:relatedTo>
        <rdfs:subPropertyOf rdf:resource="&mwjava;jndiLookupName"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#fully-qualified-interface-name -->

    <owl:DatatypeProperty rdf:about="&mwjava;fully-qualified-interface-name">
        <rdfs:label>fully-qualified-interface-name</rdfs:label>
        <rdfs:comment>Used to indicate the remote interface name</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&mwjava;jndiLookupName"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#isOptionalAttribute -->

    <owl:DatatypeProperty rdf:about="&mwjava;isOptionalAttribute">
        <rdfs:label>Is optional attribute</rdfs:label>
        <rdfs:comment> It means the attribute can be optional (true) or mandatory (false).</rdfs:comment>
        <rdfs:domain rdf:resource="&mwjava;JSPTagAttribute"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#jndiLookupName -->

    <owl:DatatypeProperty rdf:about="&mwjava;jndiLookupName">
        <rdfs:label>Jndi lookup name</rdfs:label>
        <rdfs:comment>Used to specify the JNDI name of the referenced bean</rdfs:comment>
        <rdfs:domain rdf:resource="&mwjava;EnterpriseJavaBean"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#module-name -->

    <owl:DatatypeProperty rdf:about="&mwjava;module-name">
        <rdfs:label>module-name</rdfs:label>
        <mjava:relatedTo>EJB Component</mjava:relatedTo>
        <rdfs:comment>defaults to bundle name (.war or .jar) without the bundle extension. Again, this can be overridden in ejb-jar.xml</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&mwjava;jndiLookupName"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#rtexprvalue -->

    <owl:DatatypeProperty rdf:about="&mwjava;rtexprvalue">
        <rdfs:label>Runtime expression value</rdfs:label>
        <rdfs:comment> It means the attribute can support scriptlet values.</rdfs:comment>
        <rdfs:domain rdf:resource="&mwjava;JSPTagAttribute"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#shortName -->

    <owl:DatatypeProperty rdf:about="&mwjava;shortName">
        <rdfs:label>Short name</rdfs:label>
        <mjava:relatedTo>JSP Tag Library</mjava:relatedTo>
        <skos:altLabel>prefix</skos:altLabel>
        <rdfs:domain rdf:resource="&mwjava;JSPTagLibrary"/>
        <rdfs:subPropertyOf rdf:resource="&top;name"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#standardSyntax -->

    <owl:DatatypeProperty rdf:about="&mwjava;standardSyntax">
        <rdfs:label>Standard syntax</rdfs:label>
        <mjava:relatedTo>JSP Tag Library</mjava:relatedTo>
        <rdfs:comment>The expresion to include the library in a JSP file.</rdfs:comment>
        <rdfs:domain rdf:resource="&mwjava;JSPTagLibrary"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#uri -->

    <owl:DatatypeProperty rdf:about="&mwjava;uri">
        <mjava:relatedTo>JSP Tag Library</mjava:relatedTo>
        <mjava:relatedTo>Uri</mjava:relatedTo>
        <rdfs:domain rdf:resource="&mwjava;JSPTagLibrary"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#urlPattern -->

    <owl:DatatypeProperty rdf:about="&mwjava;urlPattern">
        <rdfs:label>urlPattern</rdfs:label>
        <rdfs:comment>Describes a pattern used to resolve URLs. The portion of the URL after the http://host:port + WebAppName is compared to the &lt;url-pattern&gt; by WebLogic Server. If the patterns match, the servlet mapped in this element will be called.

Example patterns:

/soda/grape/*
/foo/* 
/contents
*.foo</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#versionJSPStandard -->

    <owl:DatatypeProperty rdf:about="&mwjava;versionJSPStandard">
        <rdfs:label>Version JSP standard</rdfs:label>
        <mjava:relatedTo>JSP Tag Library</mjava:relatedTo>
        <rdfs:domain rdf:resource="&mwjava;JSPTagLibrary"/>
        <rdfs:subPropertyOf rdf:resource="http://www.markosproject.eu/ontologies/software#version"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


