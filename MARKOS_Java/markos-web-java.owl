        <rdfs:comment>Class handle directly by the subject tag.</rdfs:comment>
        <mjava:relatedTo>JSP Tag</mjava:relatedTo>
        <rdfs:range rdf:resource="&mjava;JavaSoftwareClass"/>
        <rdfs:domain rdf:resource="&mwjava;JSPTag"/>
        <rdfs:subPropertyOf rdf:resource="http://www.markosproject.eu/ontologies/software#usedSoftwareEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#usedTagLibrary -->

    <owl:ObjectProperty rdf:about="&mwjava;usedTagLibrary">
