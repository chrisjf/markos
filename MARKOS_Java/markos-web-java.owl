    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#componentInterface -->

    <owl:ObjectProperty rdf:about="&mwjava;componentInterface">
        <rdfs:label>Component interface</rdfs:label>
        <rdfs:comment>Java interface for the business object. It defines the business logic methods, which are implemented in the bean implementation.</rdfs:comment>
        <skos:altLabel>Remote interface</skos:altLabel>
        <mjava:relatedTo>EJB</mjava:relatedTo>
        <rdfs:range rdf:resource="&mjava;JavaInterface"/>
        <rdfs:domain rdf:resource="&mwjava;EnterpriseJavaBean"/>
        <rdfs:subPropertyOf rdf:resource="http://www.markosproject.eu/ontologies/software#providedInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#definedJSPElement -->

    <owl:ObjectProperty rdf:about="&mwjava;definedJSPElement">
        <rdfs:label>Defined JSP element</rdfs:label>
        <mjava:relatedTo>JSP Page</mjava:relatedTo>
        <rdfs:comment>Define the relationshipt between a JSP scripting element and the subject JSP Page</rdfs:comment>
        <rdfs:range rdf:resource="&mwjava;JSPElement"/>
        <rdfs:domain rdf:resource="&mwjava;JSPPage"/>
        <rdfs:subPropertyOf rdf:resource="&top;subordinateEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#definedTag -->

    <owl:ObjectProperty rdf:about="&mwjava;definedTag">
        <rdfs:label>Defined tag</rdfs:label>
        <rdfs:comment>Tag defined by the subject tag library</rdfs:comment>
        <mjava:relatedTo>JSP Tag Library</mjava:relatedTo>
        <rdfs:range rdf:resource="&mwjava;JSPTag"/>
        <rdfs:domain rdf:resource="&mwjava;JSPTagLibrary"/>
        <rdfs:subPropertyOf rdf:resource="&top;subordinateEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#homeInterface -->

    <owl:ObjectProperty rdf:about="&mwjava;homeInterface">
        <rdfs:label>Home interface</rdfs:label>
        <rdfs:comment>Java interface for the Home interface.</rdfs:comment>
        <mjava:relatedTo>EJB</mjava:relatedTo>
        <rdfs:range rdf:resource="&mjava;JavaInterface"/>
        <rdfs:domain rdf:resource="&mwjava;EnterpriseJavaBean"/>
        <rdfs:subPropertyOf rdf:resource="http://www.markosproject.eu/ontologies/software#providedInterface"/>
    </owl:ObjectProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#implementationClass -->

    <owl:ObjectProperty rdf:about="&mwjava;implementationClass">
        <rdfs:label>Implementation class</rdfs:label>
        <rdfs:comment>Java class providing the implementation for the subject component.</rdfs:comment>
        <rdfs:range rdf:resource="&mjava;JavaSoftwareClass"/>
        <rdfs:subPropertyOf rdf:resource="&top;containedEntity"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&mwjava;EnterpriseJavaBean"/>
                    <rdf:Description rdf:about="&mwjava;JavaServlet"/>
                    <rdf:Description rdf:about="&mwjava;WebService"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.markosproject.eu/ontologies/markos-web-java#importedJSPProgram -->

    <owl:ObjectProperty rdf:about="&mwjava;importedJSPProgram">
        <rdfs:label>Imported JSP program</rdfs:label>
        <mjava:relatedTo>JSP Page</mjava:relatedTo>
        <rdfs:comment>The javascript file imported by the subject JSP Page</rdfs:comment>
