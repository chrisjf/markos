    <!-- http://www.markosproject.eu/ontologies/markos-web-java#JAVA_TECHNOLOGY_JAVASERVLET -->

    <owl:NamedIndividual rdf:about="&mwjava;JAVA_TECHNOLOGY_JAVASERVLET">
